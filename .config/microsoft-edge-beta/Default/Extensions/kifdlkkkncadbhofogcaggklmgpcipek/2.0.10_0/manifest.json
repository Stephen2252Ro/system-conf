{
   "author": "Guokai Han",
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "initContentScript.js" ],
      "matches": [ "*://www.bilibili.com/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": false,
      "js": [ "timeline.js" ],
      "matches": [ "https://www.bilibili.com/anime/timeline*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_extDescription__",
   "icons": {
      "128": "icon/128.png",
      "16": "icon/16.png",
      "48": "icon/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_extName__",
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "storage" ],
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "2.0.10",
   "web_accessible_resources": [ "contentScript.js" ]
}
